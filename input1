const m = 7, n = 85, t = 6;  
var  i,x,y,z,q,r;  
procedure mult(x, y);
   var a, b;
  begin 
     a := x;  b := y; z := 0;
     while b > 0 do
     begin
        if odd x then z := z+a else z := z-a;
        a := 2*a;
        b := b/2;
     end;
  end
  return a+1;

begin
  x := m;
  y := n;
  call mult(x, 8);
end.